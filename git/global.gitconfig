[user]
	email = <<EMAIL>>
	name = Yifei Yin
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	pager = less -+F -+X
	editor = vim
	excludesfile = /Users/<<USERNAME>>/.gitignore_global
[alias]
	adog = log --all --decorate --oneline --graph
	co = checkout
